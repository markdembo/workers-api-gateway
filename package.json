{
  "name": "workers",
  "version": "0.0.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "typescript": "^4.7.4",
    "wrangler": "2.0.24",
    "miniflare": "^2.5.0",
    "jest": "^28.1.0",
    "jest-environment-miniflare": "^2.5.0",
    "@types/jest": "^27.5.1",
    "ts-jest": "^28.0.3"
  },
  "private": true,
  "type": "module",
  "module": "./dist/index.mjs",
  "build": "esbuild --bundle --sourcemap --outdir=dist ./src/index.js",
  "scripts": {
    "start": "wrangler dev",
    "deploy": "wrangler publish",
